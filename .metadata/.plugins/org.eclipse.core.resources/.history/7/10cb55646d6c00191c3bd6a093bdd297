package mindTree_inheritence;

public class mindTreeJava {

	public static void main(String[] args) {

		int num = 5042;
		mindTreeJava md = new mindTreeJava();

		System.out.println(md.checkFact(num));
	}

	private boolean checkFact(int num) {
		int count = 2;

		while (num > 1) {
			if (num % 2 != 0 && num%count!=0) {
				System.out.println(num);
				return false;
			} else {
				System.out.println(num);

				num = num / count;
				count=count+1;
			}

		}
		return true;
	}

}
