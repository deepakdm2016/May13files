package HospitalUtils;

import java.util.Date;

public class patient {
	
	public static int count=0;
	
	private String name;
	private String locality;
	private Date dateOfRegistration;

	
	public Date getDateOfRegistration() {
		return dateOfRegistration;
	}




	
	patient(String name, String place, Object Date(object))
	{
		this.name=name;
		this.locality=place;
		System.out.println("Patient added "+ name + " "+place);
		this.dateOfRegistration=Date(object);
	}
	
	
	public boolean isLocalite()
	{
		if (this.locality.equals("Bangalore")) return true;
				
		return false;
		
	}
}
